{
  "name": "boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://www.example.com/app",
  "private": true,
  "engines": {
    "node": ">= 8.9",
    "yarn": ">= 1.3.2"
  },
  "pre-commit": [
    "type-check",
    "lint",
    "test"
  ],
  "scripts": {
    "analyze": "npm-run-all clean \"build:webpack --env.production --env.analyze\"",
    "build": "npm-run-all clean \"build:webpack --env.production\"",
    "build:staging": "npm-run-all clean build:webpack storybook:build",
    "build:webpack": "webpack",
    "clean": "rimraf dist",
    "lint": "tslint -t verbose -p ./",
    "start": "npm-run-all clean start:webpack",
    "start:webpack": "webpack-dev-server --env.watch",
    "storybook": "start-storybook -p 9001 -c node_modules/storybook-babel-typescript-shim",
    "storybook:build": "cross-env NODE_ENV=development build-storybook -c node_modules/storybook-babel-typescript-shim -o dist/storybook",
    "test": "jest --config jest.config.js",
    "type-check": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.44",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@babel/preset-typescript": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "@storybook/react": "^3.4.2",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.2",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack": "^4.1.3",
    "@types/webpack-bundle-analyzer": "^2.9.2",
    "@types/webpack-manifest-plugin": "^1.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-styled-components": "^1.5.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.5",
    "cross-env": "^5.1.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "react-dev-utils": "^5.0.1",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2",
    "storybook-babel-typescript-shim": "^1.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.11.0",
    "webpack-manifest-plugin": "^2.0.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  }
}
